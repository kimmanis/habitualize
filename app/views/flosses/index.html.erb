<h1>Listing flosses</h1>

<table>
  <tr>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @flosses.each do |floss| %>
  <tr>
    <td><%= floss.user %></td>
    <td><%= link_to 'Show', floss %></td>
    <td><%= link_to 'Edit', edit_floss_path(floss) %></td>
    <td><%= link_to 'Destroy', floss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Floss', new_floss_path %>
